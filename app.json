{
  "expo": {
    "name": "PantryPal",
    "slug": "pantrypal",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./src/assets/images/profile_pic.png",
    "scheme": "pantrypal",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./src/assets/images/Animatedlogo.gif",
      "resizeMode": "contain",
      "backgroundColor": "#00bf63"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./src/assets/images/profile_pic.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./src/assets/images/circleprofilepic.png"
    },
    "plugins": [
      "expo-router"
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
